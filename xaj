                                    });
                                }
                            }, this.getStatusText = function(e, t) {
                                return t.$keyChain || "";
                            };
                        }.call(s.prototype), t.HashHandler = s, t.MultiHashHandler = a;
                }), ace.define("ace/commands/command_manager", ["require", "exports", "module", "ace/lib/oop", "ace/keyboard/hash_handler", "ace/lib/event_emitter"], function(e, t, n) {
                    "use strict";
                    var i = e("../lib/oop"),
                        o = e("../keyboard/hash_handler").MultiHashHandler,
                        r = e("../lib/event_emitter").EventEmitter,
                        s = function(e, t) {
                            o.call(this, t, e), this.byName = this.commands, this.setDefaultHandler("exec", function(e) {
                                return e.command.exec(e.editor, e.args || {});
                            });
                        };
                    i.inherits(s, o),
                        function() {
                            i.implement(this, r), this.exec = function(e, t, n) {
                                if (Array.isArray(e)) {
                                    for (var i = e.length; i--;)
                                        if (this.exec(e[i], t, n)) return true;
                                    return false;
                                }
                                if ("string" === typeof e && (e = this.commands[e]), !e) return false;
                                if (t && t.$readOnly && !e.readOnly) return false;
                                if (0 != this.$checkCommandState && e.isAvailable && !e.isAvailable(t)) return false;
                                var o = {
                                    editor: t,
                                    command: e,
                                    args: n
                                };
                                return o.returnValue = this._emit("exec", o), this._signal("afterExec", o), false !== o.returnValue;
                            }, this.toggleRecording = function(e) {
                                if (!this.$inReplay) return e && e._emit("changeStatus"), this.recording ? (this.macro.pop(), this.off("exec", this.$addCommandToMacro), this.macro.length || (this.macro = this.oldMacro), this.recording = false) : (this.$addCommandToMacro || (this.$addCommandToMacro = function(e) {
                                    this.macro.push([e.command, e.args]);
                                }.bind(this)), this.oldMacro = this.macro, this.macro = [], this.on("exec", this.$addCommandToMacro), this.recording = true);
                            }, this.replay = function(e) {
                                if (!this.$inReplay && this.macro) {
                                    if (this.recording) return this.toggleRecording(e);
                                    try {
                                        this.$inReplay = true, this.macro.forEach(function(t) {
                                            "string" == typeof t ? this.exec(t, e) : this.exec(t[0], e, t[1]);
                                        }, this);
                                    } finally {
                                        this.$inReplay = false;
                                    }
                                }
                            }, this.trimMacro = function(e) {
                                return e.map(function(e) {
                                    return "string" != typeof e[0] && (e[0] = e[0].name), e[1] || (e = e[0]), e;
                                });
                            };
                        }.call(s.prototype), t.CommandManager = s;
