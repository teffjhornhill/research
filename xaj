                        }, this.animateScrolling = function(e, t) {
                            var n = this.scrollTop;
                            if (this.$animatedScroll) {
                                var i = this;
                                if (e != n) {
                                    if (this.$scrollAnimation) {
                                        var o = this.$scrollAnimation.steps;
                                        if (o.length && (e = o[0], e == n)) return;
                                    }
