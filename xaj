                        function() {
                            i.implement(this, p), this.$initOperationListeners = function() {
                                this.commands.on("exec", this.startOperation.bind(this), true), this.commands.on("afterExec", this.endOperation.bind(this), true), this.$opResetTimer = r.delayedCall(this.endOperation.bind(this, true)), this.on("change", function() {
