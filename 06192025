// index.ts
var VERSION = package_default.version;
var CLI_NAME = "pcli";
program.name("project-cli").description("A cli for managing projects").version(VERSION).action(async () => {
  if (!fs2.existsSync(configPath + "config.json")) {
    const createConfig = await esm_default2({
      message: "No cofig has been found. Would you like to generate a new one?"
    });
    if (createConfig) {
      generateDefaultConfig();
    } else {
      console.log("This project can not run without the config. Either create it yourself or generate the default.");
      exit(1);
    }
  }
  if (!getProjects() || getProjects().length == 0 || getProjects().length == undefined) {
    const anwser = await esm_default4({
      message: "Create a new Project",
      choices: ["Clone a project with git", "Create a new empty Project"]
    });
    if (anwser == "Clone a project with git") {
      const repoUrl = await esm_default3({
        message: "What is the url of the repo?"
      });
      const wantsCustomName = await esm_default2({
        message: "Would you like to give this project a custom name?",
        default: false
      });
      let customName = "";
      if (wantsCustomName) {
        customName = await esm_default3({
          message: "What would you like the custom name to be?"
        });
      }
      execSync("cd " + getConfiguration().projectsDirectory + " && " + getConfiguration().cloningCommand.replace("%s", repoUrl).replace("%n", customName));
      const newProject = {
        name: customName
      };
      addProject(newProject);
    } else if (anwser == "Create a new empty Project") {
      const name = await esm_default3({
        message: "What would you like to call the project?"
      });
      fs2.mkdirSync(getConfiguration().projectsDirectory + "/" + name);
      const newProject = {
        name
      };
      addProject(newProject);
    }
  }
  const pickedProject = await esm_default4({
    message: "Which Project would you like to go to?",
    choices: getProjects().map((project) => project.name)
  });
  console.log("__EXEC__ cd " + getConfiguration().projectsDirectory + "/" + pickedProject);
  console.log(getProjects());
});
program.command("init").action(() => {
  console.log(`${CLI_NAME}() {
    local output
    output=$(command ${CLI_NAME} "$@")
    local exit_code=$?

    if [ $exit_code -eq 0 ]; then
        echo "$output" | while IFS= read -r line; do
            if [[ $line == __EXEC__* ]]; then
                eval "\${line#__EXEC__}"
            else
                echo "$line"
            fi
        done
    else
        echo "$output" >&2
        return $exit_code
    fi
}`);
});
program.parse();
export {
  VERSION
};
